  <xs:simpleType name="day-1-31Type">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	Restrict numerical day value to 1 &lt;= day &lt;= 31
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="31"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- restricted day of week -->
  <xs:simpleType name="dow-1-7Type">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	Restrict numerical day of week value to 1 &lt;= day &lt;= 7
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:positiveInteger">
      <xs:minInclusive value="1"/>
      <xs:maxInclusive value="7"/>
    </xs:restriction>
  </xs:simpleType>

  <!--
      ***
      *** time
      ***
  -->
  <xs:complexType name="timeType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="hour" type="hour-0-23Type"/>
	  <xs:element name="minute" type="minutes-0-59Type" minOccurs="0"/>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <!-- restricted hour -->
  <xs:simpleType name="hour-0-23Type">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	Restrict numerical hour value to 0 &lt;= day &lt;= 23
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="23"/>
    </xs:restriction>
  </xs:simpleType>

  <!-- restricted minutes -->
  <xs:simpleType name="minutes-0-59Type">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	Restrict numerical minutes value to 1 &lt;= day &lt;= 59
      </xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:minInclusive value="0"/>
      <xs:maxInclusive value="59"/>
    </xs:restriction>
  </xs:simpleType>


  <!--
      ***
      *** diver
      ***
  -->
  <xs:complexType name="diverType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="owner" type="ownerType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Attribute: id=unique identifier of owner of this
		UDDF-file. Used as references in other UDDF-files
		during im- and export.
		
		Inside of the &lt;owner&gt; elementes all owners data
		is given. There can be only one (1) &lt;owner&gt;
		within the &lt; diver&gt; section.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="buddy" type="buddyType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Attribute: id=unique identifier of a dive buddy

		Inside the &lt;buddy&gt; section all known data of a certain
		dive buddy are put in brackets. Any number of &lt;buddy&gt;
		elements can be given, which describe the different
		dive buddies of the &lt;owner&gt; of the UDDF file.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <!-- owner and buddy -->

  <xs:complexType name="ownerType">
    <xs:complexContent>
      <xs:extension base="personType">
	<!-- additional owner elements -->
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="buddyType">
    <xs:complexContent>
      <xs:extension base="personType">
	<xs:sequence>
	  <!-- additional buddy elements -->
	  <xs:element name="student" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		student of owner?
	      </xs:documentation>
	    </xs:annotation>
	    <xs:complexType>
	    </xs:complexType>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="individualType">
    <xs:complexContent>
      <xs:annotation>
	<xs:documentation xml:lang="en">
	</xs:documentation>
      </xs:annotation>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="personal" type="personalType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;personal&gt; names, titles, sex and
		birthdate can be given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="address" type="addressType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;address&gt; the own (&lt;owner&gt; element)
		address data, or of dive buddies (&lt;buddy&gt;
		element), or of a shop (&lt;shop&gt; element)
		respectively, are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="contact" type="contactType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;contact&gt; data concerning getting in
		touch with some one are given, like phone number,
		email address, language etc.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="personType">
    <xs:complexContent>
      <xs:annotation>
	<xs:documentation xml:lang="en">
	  other order of elements for shop, etc?
	</xs:documentation>
      </xs:annotation>
      <xs:extension base="individualType">
	<xs:sequence>
	  <xs:element name="equipment" type="equipmentType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside the &lt;equipment&gt; section all data
		concerning the diving equipment are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="medical" type="medicalType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;medical&gt; medical data are stored.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <!-- @@@ clearify: certification in docs? is sibling of education! -->
	  <xs:element name="education" type="educationType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;education&gt; information concerning the
		educational level of the diver is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="divepermissions" type="divepermissionsType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;divepermissions&gt; data concerning dive
		permissions are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="diveinsurances" type="diveinsurancesType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;diveinsurances&gt; data concerning taken
		out dive insurance policies are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="notes" type="notesType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;notes&gt; additional data like text, links
		to web-sites and pictures can be given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="personalType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="firstname" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;firstname&gt; the first name of a person is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="middlename" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;middlename&gt; the second Christian name of a
		person is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="lastname" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;lastname&gt; a person's last name is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="title" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;title&gt; all titles of a person are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="sex" type="sexType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;sex&gt; the sex of a person is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="birthdate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;birthdate&gt; the date of birh of a person
		is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="passport" type="passportType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;passport&gt; the number of the passport is
		put into brackets.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:simpleType name="sexType">
    <xs:restriction base="xs:string">
      <!-- underterminated -->
      <xs:enumeration value="0"/>
      <!-- male -->
      <xs:enumeration value="1"/>
      <xs:enumeration value="m"/>
      <xs:enumeration value="male"/>
      <!-- female -->
      <xs:enumeration value="2"/>
      <xs:enumeration value="f"/>
      <xs:enumeration value="female"/>
      <!-- hermaphrodite -->
      <xs:enumeration value="3"/>
      <xs:enumeration value="h"/>
      <xs:enumeration value="hermaphrodite"/>
    </xs:restriction>
  </xs:simpleType>


  <xs:complexType name="birthdateType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="date" type="dateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="addressType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="street" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;street&gt; the name of a street in an
		address is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="city" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;city&gt; the city of an address is named.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="postcode" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;postcode&gt; the city's postcode is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="province" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="de">
		Mit &lt;province&gt; wird die Bezeichnung der Provinz
		oder eines Bundeslandes usw. geklammert.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="country" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;country&gt; the country of a city, or dive
		spot respectively, are put in brackets.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="passportType">
    <xs:simpleContent>
      <xs:extension base="xs:string"/>
    </xs:simpleContent>
  </xs:complexType>


  <xs:complexType name="contactType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="language" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;language&gt; the native language of a
		person is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="phone" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;phone&gt; the phone number of a person is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="mobilephone" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;mobilephone&gt; the number of a mobilephone
		is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="fax" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;fax&gt; indicates the fax number.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="email" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;email&gt; the email address of a person is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="homepage" type="xs:anyURI" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;homepage&gt; puts the URL (Unified Resource
		Locator) of a homepage in brackets.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
