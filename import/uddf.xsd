	  <xs:element name="manufacturer" type="manufacturerType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Within &lt;manufacturer&gt; the manufacturer of a
		product is given.
	      </xs:documentation>
	    </xs:annotation>
