  <xs:complexType name="dcdivepo2alarmType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="maximumpo2" type="xs:float">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Indicates the - more or less arbitrarily chosen -
		threshold for the oxygen partial pressure, when the
		oxygen fraction of this breathing gas starts to be
		poisonous. The value is given as a real number,
		normally it ranges between 1.4 and 1.7.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="dcalarm" type="dcalarmType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		By means of &lt;dcalarm&gt; the alarm type for a
		certain event is defined which a dive computer has to
		generate.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="dcdivesitedataType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:attribute name="site" type="xs:IDREF" use="required">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	      unique identifier of the site which data are to be
	      transferred to the dive computer
	    </xs:documentation>
	  </xs:annotation>
	</xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="dcbuddydataType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:attribute name="buddy" type="xs:IDREF" use="required">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	      unique identifier of the buddy whose data are to be
	      transferred
	    </xs:documentation>
	  </xs:annotation>
	</xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="dcdivedepthalarmType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="dcalarmdepth" type="xs:float">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		a dive depth in metres (given as a real number) is set
		at which the dive computer shall give a certain alarm.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="dcalarm" type="dcalarmType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		By means of &lt;dcalarm&gt; the alarm type for a
		certain event is defined which a dive computer has to
		generate.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="dcdivetimealarmType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="minute" type="xs:positiveInteger">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		dive time in minutes is set when the dive computer
		shall give an alarm signal
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="dcalarm" type="dcalarmType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		By means of &lt;dcalarm&gt; the alarm type for a
		certain event is defined which a dive computer has to
		generate.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  

  <xs:complexType name="dcalarmtimeType">
    <xs:complexContent>
      <xs:extension base="timeType">
	<xs:sequence>
	  <xs:element name="dcalarm" type="dcalarmType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		By means of &lt;dcalarm&gt; the alarm type for a
		certain event is defined which a dive computer has to
		generate.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  

  <xs:simpleType name="dcalarmType">
    <xs:restriction base="xs:nonNegativeInteger">
      <xs:annotation>
	<xs:documentation xml:lang="en">
	  Further documentation required.
	</xs:documentation>
      </xs:annotation>
    </xs:restriction>
  </xs:simpleType>
  

  <xs:complexType name="getdcdataType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="getdcalldata" type="simpleTagType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for all data stored on the dive computer
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcgeneratordata" type="simpleTagType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en"> request for the
	      manufacturer's data (see also &lt;generator&gt;
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcownerdata" type="simpleTagType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for the owner's data (&lt;owner&gt;) (see also
		&lt;diver&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcbuddydata" type="simpleTagType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for all dive buddy data (&lt;buddy&gt;) (see
		also &lt;diver&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcgasdefinitionsdata" type="simpleTagType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for all stored breathing gas data (see also
		&lt;gasdefinitions&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcdivesitedata" type="simpleTagType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for descriptions of dive spots (see also
		&lt;divesite&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcdivetripdata" type="simpleTagType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for descriptions of dive trips (see also
		&lt;divetrip&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcprofiledata" type="simpleTagType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		request for dive profile data (see also
		&lt;profiledata&gt;)
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="simpleTagType">
  </xs:complexType>


  <!-- ********** ********** ********** ********** -->
  <!-- ********** ********** ********** ********** -->

  <!-- ************* -->
  <!-- general types -->
  <!-- ************* -->

  <xs:complexType name="referenceType">
    <xs:annotation>
      <xs:documentation xml:lang="en">
	The type referenceType is general type with the proper
	attribute for all types of referneces.
      </xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:attribute name="ref" type="xs:IDREF" use="required">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	      The attribte ref references an element with the proper
	      id attribute.
	    </xs:documentation>
	  </xs:annotation>
	</xs:attribute>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="priceType">
    <xs:simpleContent>
      <xs:extension base="xs:float">
	<xs:attribute name="currency" type="xs:string"
		      use="optional">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	      eindeutiger Bezeichner einer Währung (sollte
	      drei Buchstaben lang sein)
	    </xs:documentation>
	  </xs:annotation>
	</xs:attribute>
      </xs:extension>
    </xs:simpleContent>
  </xs:complexType>


  <xs:complexType name="manufacturerType">
    <xs:complexContent>
      <xs:extension base="namedType">
	<xs:sequence>
	  <xs:element name="address" type="addressType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;address&gt; the own (&lt;owner&gt; element) address
		data, or of dive buddies (&lt;buddy&gt; element), or of a
		shop (&lt;shop&gt; element) respectively, are given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="contact" type="contactType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;contact&gt; data concerning getting in touch with
		some one are given, like phone number, email address,
		language etc.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <!-- ********** ********** ********** ********** -->
  <!--
      ***
      *** Yes, the following types are all identical.
      *** But I introduced them in case that the type changes for some reason.
      ***
  -->

  <xs:complexType name="versionType">
    <xs:simpleContent>
      <xs:extension base="xs:string"/>
    </xs:simpleContent>
  </xs:complexType>


</xs:schema>
