      <xs:extension base="namedType">
	<xs:sequence>
	  <xs:element name="issuedate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;issuedate&gt; the date of issue of a level is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="validdate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;issuedate&gt; the date of issue of a level is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="notes" type="notesType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;notes&gt; können zusätzliche Daten, wie
