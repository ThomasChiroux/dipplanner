  <xs:complexType name="medicalType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="examination" type="examinationType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;examination&gt; data about the divers
		medical state is stored.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="educationType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="certification" type="certificationType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;certification&gt; data about the
		certification of a diver is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="certificationType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:choice>
	    <xs:element name="level" type="xs:string" minOccurs="0">
	      <xs:annotation>
		<xs:documentation xml:lang="en">
		  With &lt;level&gt; a dive level is put into
		  brackets, for example Bronze, Silver, Gold, or OWD,
		  AOWD, Divemaster etc.
		</xs:documentation>
	      </xs:annotation>
	    </xs:element>
	    <xs:element name="specialty" type="xs:string"
			minOccurs="0">
	      <xs:annotation>
		<xs:documentation xml:lang="en">
		  Inside &lt;specialty&gt; the name of a specialty is
		  put into brackets.
		</xs:documentation>
	      </xs:annotation>
	    </xs:element>
	  </xs:choice>

	  <xs:element name="organization" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Mit &lt;organization&gt; wird der Tauchverband
		angegeben, bei dem die entsprechende Brevetierung
		erfolgte.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="instructor" type="instructorType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;instructor&gt; information about the dive
		instructor is given, who issued the level.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="issuedate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;issuedate&gt; the date of issue of a level
		is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="validdate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;validdate&gt; the date is given until a brevet is
		valid (if existent).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="notes" type="notesType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  

  <xs:complexType name="instructorType">
    <xs:complexContent>
      <xs:extension base="individualType">
	<xs:sequence>
	  <xs:element name="notes" type="notesType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="divepermissionsType">
