  <xs:complexType name="profileType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <!--
	  <xs:element name="diveref" type="referenceType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
