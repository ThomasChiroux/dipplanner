  <!--
      ***
      *** divecomputercontrolType
      ***
  -->
  <xs:complexType name="divecomputercontrolType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="setdcdata" type="setdcdataType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside the &lt;setdcdata&gt; section follows
		information concerning the configuration of a dive
		computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="getdcdata" type="getdcdataType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		By means of &lt;getdcdata&gt; the dive computer is
		instructed to send data to the calling software. If
		there are no further elements inside the
		&lt;getdcdata&gt; section, all data - of all stored
		dives - are transfered.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="setdcdataType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="setdcpassword" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Via &lt;setdcpassword&gt; a password is set on the
		dive computer by the owner to protect the data against
		unautorized access. But How?
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcownerdata" type="simpleTagType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Via &lt;setdcownerdata&gt; all data of the owner of
		the dive computer which are stored inside the
		&lt;owner&gt; section, are transferred to the dive
		computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcbuddydata" type="dcbuddydataType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Via &lt;setdcbuddydata&gt; all data of the specified
		dive buddy which are stored inside the &lt;buddy&gt;
		section, are transferred to the dive computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>

	  <xs:element name="setdcdate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;setdcdate&gt; sets the date in the dive computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdctime" type="encapsulatedTimeType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;setdcdate&gt; sets the date in the dive computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>

	  <xs:element name="setdcalarmtime" type="dcalarmtimeType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		With &lt;setdcalarmtime&gt; it is possible to set a -
		absolute (independent from dive time) - time when the
		dive computer shall give an alarm.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcdivesitedata" type="dcdivesitedataType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Via &lt;setdcdivesitedata&gt; all data given inside
		the &lt;divesite&gt; section concerning the specified
		- via the attribute - dive site (&lt;site&gt;) are
		transferred to the dive computer.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcdivetimealarm" type="dcdivetimealarmType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		With &lt;setdcdivetimealarm&gt; the dive time in
		minutes is set when the dive computer shall give an
		alarm signal.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcdivedepthalarm" type="dcdivedepthalarmType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		With &lt;setdcdivedepthalarm&gt; a dive depth in
		metres (given as a real number) is set at which the
		dive computer shall give a certain alarm.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcdivepo2alarm" type="dcdivepo2alarmType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;setdcdivepo2alarm&gt; allows for setting an oxygen
		partial pressure at which the dive computer shall give
		an alarm signal.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcendndtalarm" type="dcendndtalarmType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		&lt;setdcendndtalarm&gt; (,,set dive computer end no
		decompression time alarm``) tells the dive computer to
		give an alarm signal when the end of the no
		decompression time is reached.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcdecomodel" type="dcdecomodelType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Sets the decompression model to be used in the dive
		computer. This element is for future use.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcaltitude" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		With &lt;setdcaltitude&gt; the altitude in metres
		above sea level (given as a real number) can be set in
		the dive computer for the next dive. This element
		serves for an easy way to set the altitude for a dive
		spot in the dive computer without cross referencing to
		a previous made data base for this lake, or river.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="setdcproprietarydata" type="dcproprietarydataType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;setdcproprietarydata&gt; proprietary data
		of a dive computer manufacturer can be given. The
		elements inside &lt;setdcproprietarydata&gt; are
		neglected by other dive computers.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <!--
	  <xs:element name="" type="Type" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  -->
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="dcproprietarydataType" mixed="true">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
