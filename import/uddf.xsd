		</xs:documentation>
	      </xs:annotation>
	    </xs:element>
	  </xs:choice>

	  <xs:choice>
	    <xs:element name="individualtrip" minOccurs="0">
	      <xs:annotation>
		<xs:documentation xml:lang="en">
