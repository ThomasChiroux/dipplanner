  <xs:complexType name="tripType">
    <xs:complexContent>
      <xs:extension base="namedType">
	<xs:sequence>
	  <xs:element name="startdate" type="encapsulatedDateType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;startdate&gt; the date of the beginning of a dive
		trip is given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="enddate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;enddate&gt; the date of the end of a dive trip is
		given. See also &lt;startdate&gt;.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="geography" type="geographyType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
