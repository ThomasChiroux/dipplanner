  <xs:complexType name="permitType">
    <xs:complexContent>
      <xs:extension base="namedType">
	<xs:sequence>
	  <xs:element name="region" type="xs:string" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;region&gt; wird die Region einer Taucherlaubnis
		angegeben.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="issuedate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;issuedate&gt; the date of issue of a level is
		given.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="validdate" type="encapsulatedDateType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;validdate&gt; the date is given until a brevet is
		valid (if existent).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="notes" type="notesType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;notes&gt; können zusätzliche Daten, wie
		z.B. Anmerkungen (Text), Verweise zu Heimseiten
		und/oder Fotos angegeben werden.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="diveinsurancesType">
