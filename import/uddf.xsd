  <!--
      *** ********** ********** ********** **********
      *** notes stuff
      ***
  -->

  <xs:complexType name="notesType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="audio" type="audioType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		With &lt;audio&gt; an audio sequence is put in
		brackets.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="image" type="imageType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;image&gt; data concerning a picture - whatever
		type of - is put into brackets. How the image data
		are displayed is in the responsibility of the
		application program, also whether the software is
		able to display the image data, anyhow. To assist
		the application program the height (height), the
		width (width), and the format (format) of the image
		to be displayed can be given as attributes. The
		statement can be repeated any times.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="video" type="videoType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;video&gt; a video sequence is put into
		brackets.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="text" type="notesTextType"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;text&gt; general information is given which is
		to be interpreted as "normal" text.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="notesTextType" mixed="true">
    <xs:annotation>
      <xs:documentation xml:lang="en">
      </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:any minOccurs="0" maxOccurs="unbounded" processContents="skip"/>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="audioType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="objectname" type="objectnameType">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Inside &lt;objectname&gt; the name of a picture, or audio,
		or video file respectively, is put into
		brackets. Paths are to be given in Unix notation with
		a slash "/" as divisor between directories. It is both
		possible to give either an absolute path, or a
		relative one respectively.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
