  <!--
      ***
      *** gasdefinitons
      ***
  -->
  <xs:complexType name="gasdefinitionsType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="mix" type="mixType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		The information put into brackets within &lt;mix&gt;
		define a breathing gas used. If more than one
		breathing gas is used, several &lt;mix&gt; sections
		follow one after the other.

		Within following &lt;tankdata&gt; sections
		cross-references can be made to breathing gases
		defined here.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="tankdata" type="tankdataType"
		      minOccurs="0" maxOccurs="unbounded">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Attribute: id=unique identifier for this tank used

		Inside the &lt;tankdata&gt; section information
		concerning a breathing gas consumption calculation is
		put into brackets. As many as needed &lt;tankdata&gt;
		elements can appear one after the other, but all have
		to stand after the &lt;mix&gt; elements.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="mixType">
    <xs:complexContent>
      <xs:extension base="namedType">
	<xs:sequence>
	  <xs:element name="o2" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Gibt den Sauerstoff-Anteil des Atemgases an. Die
		Angabe erfolgt als (Volumen-)Prozentangabe, wobei die
		Prozentangabe als reelle Zahl kleiner oder gleich 1.0
		erfolgt.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="n2" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Defines the nitrogen fraction of a breathing gas,
		given as a real number less or equal 1.0 in percent
		(of volume).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="he" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		The fraction of Helium in the breathing gas, given as
		a real number less or equal 1.0 in percent (of
		volume).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="ar" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Specifies the argon fraction of a (breathing) gas,
		given as a real number less or equal 1.0 in percent
		(of volume).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="h2" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		The fraction of Hydrogen in the breathing gas, given
		as a real number less or equal 1.0 in percent (of
		volume).
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="priceperlitre" type="priceType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Mit &lt;priceperlitre&gt; wird der Preis als reelle Zahl mit
		zwei Nachkommastellen für einen (1) Liter eines
		Atemgases geklammert.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:choice>
	    <xs:element name="maximumpo2" type="xs:float" minOccurs="0">
	      <xs:annotation>
		<xs:documentation xml:lang="en">
		  Gibt den - mehr oder weniger willkürlich gewählten -
		  Grenzwert des Sauerstoff-Partialdrucks an, ab dem
		  der Sauerstoff-Gehalt dieses Atemgases giftig
		  wird. Die Angabe erfolgt als reelle Zahl mit einer
		  Nachkommastelle. Gebräuchliche Werte liegen zwischen
		  1.4 und 1.7.
		</xs:documentation>
	      </xs:annotation>
	    </xs:element>
	    <xs:element name="maximumoperationdepth" type="xs:float"
			minOccurs="0">
	      <xs:annotation>
		<xs:documentation xml:lang="en">
		  Defines the maximum operation depth (MOD) of a
		  breathing gas, given as a real number.
		</xs:documentation>
	      </xs:annotation>
	    </xs:element>
	  </xs:choice>
	  <xs:element name="equivalentairdepth" type="xs:float"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		The "equivalent air depth" of a breathing gas, given
		as a real number.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


  <xs:complexType name="tankdataType">
    <xs:complexContent>
      <xs:extension base="UDDF_ROOT">
	<xs:sequence>
	  <xs:element name="mixref" type="referenceType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="tankref" type="referenceType" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="tankvolume" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Volume of the tank used in cubicmetres [m^3] - not in
		litres, as UDDF uses SI units!
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="tankpressurebegin" type="xs:float" minOccurs="1">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Anfangs-Fülldruck in Pascal der Flasche.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <xs:element name="tankpressureend" type="xs:float" minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		End-Fülldruck der Flasche in Pascal nach Beendigung
		des Tauchgangs.
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	  <!-- or breathingconsumptionvolume -->
	  <xs:element name="breathingconsumptionvolume" type="xs:float"
		      minOccurs="0">
	    <xs:annotation>
	      <xs:documentation xml:lang="en">
		Indicates the breathing gas consumption volume for
		this breathing gas. The unit to be used is cubicmeter
		per second [m^3/s]! A conversion into the unit "litre
		per minute" which is normally used in dive tables is
		subject to the executing software - UDDF uses SI units
		only!
	      </xs:documentation>
	    </xs:annotation>
	  </xs:element>
	</xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>


