	    <xs:documentation xml:lang="en">
	    </xs:documentation>
	  </xs:annotation>
	</xs:attribute>
	<xs:attribute name="format" type="xs:string" use="optional">
	  <xs:annotation>
	    <xs:documentation xml:lang="en">
	    </xs:documentation>
