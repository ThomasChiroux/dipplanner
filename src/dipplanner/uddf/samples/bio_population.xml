            <sitedata>
                <minimumdepth>18.0</minimumdepth>
                <maximumdepth>32.0</maximumdepth>
                <arealength>20.0</arealength>         <!-- length of examined rock -->
                <areawidth>8.0</areawidth>            <!-- width of rock -->
